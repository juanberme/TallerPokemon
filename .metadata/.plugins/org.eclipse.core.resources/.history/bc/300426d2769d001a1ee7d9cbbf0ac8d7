package model;

public abstract class Entidades implements Runnable{

	private boolean arribita,abajito,derechita,izquier;
	
	
	
	
	
	public Entidades() {
		
	arribita=false;
	abajito=false;
	derechita=false;
	izquier=false;
		
		
	}
	
	public void draw() {
		
		
		
		
	}
	
	public void movimientoarriba() {
		
		
		
		
		
	}
	public void movimientoabajo() {
		
		
		
		
		
	}
	public void movimientoladod() {
		
		
		
		
		
	}
	public void movimientoladoi() {
		
		
		
	}
	
	@Override
	public void run() {
		
		
		
		
		movimientoabajo();
		movimientoarriba();
		movimientoladod();
		movimientoladoi();
		
	}
	
	
}
